{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "../../src/",
            "../../plugins/fbinstant/src"
        ],
        "exclude": [
            "../../src/phaser-arcade-physics.js",
            "../../src/phaser-core.js",
            "../../src/physics/matter-js/poly-decomp/",
            "../../src/physics/matter-js/lib",
            "../../src/polyfills",
            "../../src/layer3d"
        ],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["./jsdoc-plugins/typedef"],
    "opts": {
        "debug": true,
        "template": "./bin/",
        "encoding": "utf8",
        "destination": "../../types/",
        "outputSourceFiles": false,
        "outputSourcePath": true,
        "recurse": true,
        "private": false,
        "lenient": true,
        "sourceType": "script"
    }
}
